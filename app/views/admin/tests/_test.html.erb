<tr>
  <td><%= test.name %></td>
  <td><%= test.level %></td>
  <td><%= t('admin.tests.test.questions_count', count: test.questions.count) %></td>
  <td><%= link_to t('admin.tests.test.show'), admin_test_path(test) %></td>
  <td><%= link_to t('admin.tests.test.edit'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('admin.tests.test.delete'),
        admin_test_path(test),
        method: :delete,
        data: { confirm: t('admin.tests.test.confirm_delete') }
    %>
  </td>
</tr>
