<div class="form-group">
  <%= form_with url: sessions_path, local: true do |form| %>
    <div>
      <%= form.label :email %><br>
      <%= form.text_field :email, id: :email, class: 'form-control', placeholder: 'Enter email' %>
    </div>

    <div>
      <%= form.label :password %><br>
      <%= form.password_field :password, id: :password, autocomplete: "off", class: 'form-control' %>
    </div>

    <div><%= form.submit 'Enter', class: 'btn btn-primary' %></div>
  <% end %>
</div>
